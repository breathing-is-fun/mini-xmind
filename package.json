{
  "name": "mini-xmind",
  "version": "1.1.3",
  "scripts": {
    "start": "nino go -e ./src/go.tsx",
    "test": "nino test",
    "build": "nino koei -c ./scripts/nino.koei.js",
    "compile": "nino compile",
    "dev": "nino koei -w-d",
    "lint": "npx eslint .",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org && npm publish && npm config set registry=https://registry.npm.taobao.org",
    "pub": "npm run build && npm run compile && git push origin master && npm publish"
  },
  "description": "A tool to make mind-mapping easier",
  "files": [
    "lib",
    "dist"
  ],
  "license": "MIT",
  "main": "lib/index",
  "dependencies": {
    "antd": "^3.13.1",
    "dekko": "^0.2.1",
    "omit.js": "^1.0.0",
    "react-draggable": "^3.1.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "eslint": "^5.12.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "nino-cli": "^0.4.2",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3",
    "ts-loader": "^5.3.3"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "npm run prettier",
      "git add"
    ]
  }
}
