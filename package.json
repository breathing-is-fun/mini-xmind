{
  "name": "mini-xmind",
  "version": "1.1.0",
  "scripts": {
    "start": "nino go -e ./src/go.js",
    "test": "nino test",
    "test:u": "nino test -u",
    "build": "nino koei -c ./scripts/nino.koei.js",
    "compile": "nino compile",
    "dev": "nino koei -w-d",
    "lint": "npx eslint .",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org && npm publish && npm config set registry=https://registry.npm.taobao.org",
    "pub": "git push origin master && npm publish"
  },
  "license": "MIT",
  "main": "lib/index",
  "dependencies": {
    "antd": "^3.13.1",
    "dekko": "^0.2.1",
    "nino-cli": "^0.3.0",
    "omit.js": "^1.0.0",
    "react-draggable": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,css}": [
      "npm run prettier",
      "npx eslint .",
      "git add"
    ]
  }
}
