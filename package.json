{
  "name": "mini-xmind",
  "version": "1.3.1",
  "description": "A tool to make mind-mapping easier",
  "license": "MIT",
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "sideEffects": [
    "*.css"
  ],
  "main": "lib",
  "module": "es",
  "scripts": {
    "build": "nino koei",
    "clean": "rimraf node_modules package-lock.json dist es lib && npm i",
    "codecov": "nino test --coverage",
    "compile": "nino compile",
    "deploy": "node scripts/publish",
    "lint": "nino eslint",
    "lint-staged": "lint-staged",
    "prettier": "nino prettier",
    "pub": "npm run build && npm run compile && git push origin master && npm publish",
    "sort": "npx sort-package-json",
    "start": "nino go",
    "test": "nino test --testPathPattern='src'"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0-alpha.8",
    "antd": "^4.0.0-alpha.2",
    "core-js": "^3.1.4",
    "nino-cli": "^1.1.1",
    "react-draggable": "^3.3.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "dekko": "^0.2.1",
    "gh-pages": "^2.1.0",
    "lint-staged": "^9.2.1",
    "mockdate": "^2.0.5",
    "typescript": "^3.5.3"
  }
}
