{
  "name": "mini-xmind",
  "version": "1.3.0",
  "scripts": {
    "start": "nino go -e go",
    "test": "nino test",
    "build": "nino koei -c scripts/nino.koei.js",
    "compile": "nino compile",
    "codecov": "nino test --coverage",
    "lint": "tslint -c tslint.json 'src/**/*.ts[x]'",
    "prettier": "nino prettier",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org && npm publish && npm config set registry=https://registry.npm.taobao.org",
    "pub": "npm run build && npm run compile && git push origin master && npm publish",
    "pre-deploy": "node scripts/copyCircleCiConfig.js",
    "deploy": "node scripts/publish"
  },
  "description": "A tool to make mind-mapping easier",
  "files": [
    "lib",
    "dist",
    "es"
  ],
  "license": "MIT",
  "main": "lib",
  "module": "es",
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "antd": "^3.20.3",
    "react-draggable": "^3.3.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "core-js": "^3.1.4",
    "dekko": "^0.2.1",
    "gh-pages": "^2.0.1",
    "lint-staged": "^9.2.0",
    "nino-cli": "^1.0.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.3",
    "walk": "^2.3.14"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "npm run lint",
      "git add"
    ]
  }
}
