{
  "name": "mini-xmind",
  "version": "0.0.0",
  "scripts": {
    "start": "nino go",
    "test": "nino test",
    "test:u": "nino test -u",
    "build": "nino koei",
    "dev": "nino koei -w-d",
    "lint": "npx eslint .",
    "prettier": "node ./scripts/prettier.js",
    "lint-staged": "lint-staged"
  },
  "license": "MIT",
  "dependencies": {
    "antd": "^3.12.1",
    "nino-cli": "^0.1.1",
    "omit.js": "^1.0.0",
    "react-draggable": "^3.1.1"
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.15.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{js,css}": [
      "node ./scripts/prettier.js",
      "npx eslint .",
      "git add"
    ]
  }
